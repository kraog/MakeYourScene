<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="kraog.moveyourscene">

<uses-permission android:name="android.permission.INTERNET"/>

<application
    android:allowBackup="true"
    android:icon="@drawable/euskobands_ico"
    android:label="@string/app_name"
    android:largeHeap="true"
    android:theme="@style/AppTheme">
    <activity
        android:name=".view.BandDetailActivity"
        android:parentActivityName=".view.BandListActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".view.BandListActivity"/>
    </activity>
    <activity
        android:name=".view.BandListActivity"
        android:label="@string/app_name">

        <intent-filter>
            <action android:name="android.intent.action.MAIN"/>

            <category android:name="android.intent.category.LAUNCHER"/>
        </intent-filter>
    </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
    <meta-data
        android:name="com.google.android.gms.version"
        android:value="@integer/google_play_services_version" />

</application>

</manifest>
